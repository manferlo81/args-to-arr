{
  "name": "args-to-arr",
  "version": "1.4.8",
  "description": "converts arguments or any other array-like object into an array starting from specific index.",
  "main": "dist/args-to-arr.cjs.js",
  "module": "dist/args-to-arr.es.js",
  "browser": "dist/args-to-arr.umd.js",
  "types": "types/index.d.ts",
  "typings": "types/index.d.ts",
  "bundlib": {
    "name": "toArray",
    "equals": true,
    "min": "browser"
  },
  "keywords": [
    "arguments",
    "params",
    "array",
    "array-like",
    "to-array",
    "convert",
    "rest",
    "slice"
  ],
  "scripts": {
    "clean": "rm -rf node_modules/.cache/bundlib node_modules/.cache/jest dist types coverage",
    "lint": "eslint src/**/*.ts",
    "dev": "bundlib -dw",
    "build": "bundlib -d",
    "test": "jest --color -w 3",
    "prerelease": "npm run clean && npm test && npm run lint && npm run build",
    "release": "standard-version"
  },
  "files": [
    "dist",
    "types"
  ],
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "is-array-like": "^1.1.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.22",
    "@types/node": "^12.12.7",
    "bundlib": "^0.13.1",
    "chokidar": "^3.3.0",
    "eslint": "^6.6.0",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "standard-version": "^7.0.0",
    "ts-jest": "^24.1.0",
    "typescript": "^3.7.2"
  },
  "author": {
    "name": "Manuel Fernandez",
    "email": "manferlo81@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manferlo81/args-to-arr.git"
  },
  "bugs": {
    "url": "https://github.com/manferlo81/args-to-arr/issues"
  },
  "homepage": "https://github.com/manferlo81/args-to-arr#readme",
  "license": "MIT"
}
